# git
alias g="git"
alias gs="git status"
alias gd="git diff"
alias gl="git log --graph --decorate --all"

alias gcb="git checkout"
alias gcnb="git checkout -b"
alias gpnb="git push -u origin"
alias gps="git push"
alias gp="git pull"
alias gaa="git add all"
