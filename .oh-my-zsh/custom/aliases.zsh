# zsh
alias so="source ~/.zshrc"
alias cl="clear"

# vi
alias v="nvim"
alias vi="nvim"
alias vim="nvim"
alias nv="nvim"
alias nvi="nvim"

# tmux
alias t="tmux"
alias tl="tmux ls"
alias ta="tmux attach"
alias tk="tmux kill-session"
